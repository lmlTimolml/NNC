{
    "arrowParens": "avoid",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": true,
    "proseWrap": "always",
    "printWidth": 80,
    "bracketSameLine": true,
    "bracketSpacing": true,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}